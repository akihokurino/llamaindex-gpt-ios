{
  "family": "my-gpt-api",
  "executionRoleArn": "arn:aws:iam::326914400610:role/my-gpt-api-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::326914400610:role/my-gpt-api-ecs-task-role",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "main",
      "image": "326914400610.dkr.ecr.ap-northeast-1.amazonaws.com/my-gpt:local-202306050022",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/my-gpt-api",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "ap-northeast-1"
        },
        {
          "name": "PORT",
          "value": "80"
        }
      ],
      "command": ["python", "main.py"]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}